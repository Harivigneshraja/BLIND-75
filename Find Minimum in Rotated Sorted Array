import java.util.*;
public class Main {
    public static int findMin(int[] nums) {
        Arrays.sort(nums);
        return nums[0];
    }
    public static void main(String[]args)
    {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=sc.nextInt();
        }
        int res=findMin(a);
        System.out.println(res);
    }
}
